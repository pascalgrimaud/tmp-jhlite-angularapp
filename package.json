{
  "name": "angularapp",
  "version": "0.0.0",
  "private": true,
  "description": "AngularApp Project",
  "license": "UNLICENSED",
  "scripts": {
    "build": "ng build --output-path=target/classes/static",
    "ng": "ng",
    "prepare": "husky install",
    "prettier:check": "prettier --check \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "start": "ng serve",
    "test": "ng test --coverage",
    "watch": "ng build --watch --configuration development"
  },
  "dependencies": {
    "@angular/animations": "13.2.2",
    "@angular/common": "13.2.2",
    "@angular/compiler": "13.2.2",
    "@angular/core": "13.2.2",
    "@angular/forms": "13.2.2",
    "@angular/platform-browser": "13.2.2",
    "@angular/platform-browser-dynamic": "13.2.2",
    "@angular/router": "13.2.2",
    "rxjs": "7.5.5",
    "tslib": "2.3.1",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-builders/jest": "13.0.3",
    "@angular-devkit/build-angular": "13.3.0",
    "@angular/cli": "13.3.0",
    "@angular/compiler-cli": "13.2.2",
    "@prettier/plugin-xml": "1.2.0",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.1",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "12.3.7",
    "prettier": "2.6.0",
    "prettier-plugin-java": "1.6.1",
    "prettier-plugin-packagejson": "2.2.16",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "jestSonar": {
    "reportPath": "target/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  }
}
